<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#B3273149" />

            <stroke android:width="1dp" android:color="#B32F6699" />
        </shape></item>
    <item><shape>
            <gradient android:angle="270" android:endColor="#131824" android:startColor="#6C8CBD"></gradient>

            <stroke android:width="1dp" android:color="#B32F6699" />
        </shape></item>

</selector>